	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">

	<id>web</id>

	<formats>
		<format>war</format>
	</formats>

	<includeBaseDirectory>false</includeBaseDirectory>

	<fileSets>
		<fileSet>
			<directory>../registrationManager-web/target/registrationManager-web</directory>
			<outputDirectory>/</outputDirectory>
			<excludes>
				<exclude>WEB-INF/classes/bean/**</exclude>
				<exclude>WEB-INF/lib/**</exclude>
				<exclude>WEB-INF/classes/logback.xml</exclude>
				<exclude>WEB-INF/web.xml</exclude>
			</excludes>
			<fileMode>0644</fileMode>
			<directoryMode>0755</directoryMode>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>src/resources/bean/common</directory>
			<outputDirectory>WEB-INF/classes/bean</outputDirectory>
			<includes>
				<include>**</include>
			</includes>
			<fileMode>0644</fileMode>
			<directoryMode>0755</directoryMode>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>src/resources/bean/web</directory>
			<outputDirectory>WEB-INF/classes/bean</outputDirectory>
			<includes>
				<include>**</include>
			</includes>
			<fileMode>0644</fileMode>
			<directoryMode>0755</directoryMode>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>src/resources/log/web</directory>
			<outputDirectory>WEB-INF/classes</outputDirectory>
			<includes>
				<include>**</include>
			</includes>
			<fileMode>0644</fileMode>
			<directoryMode>0755</directoryMode>
			<filtered>true</filtered>
		</fileSet>
		<fileSet>
			<directory>src/resources/web/web</directory>
			<outputDirectory>WEB-INF</outputDirectory>
			<includes>
				<include>**</include>
			</includes>
			<fileMode>0644</fileMode>
			<directoryMode>0755</directoryMode>
			<filtered>false</filtered>
		</fileSet>
	</fileSets>

